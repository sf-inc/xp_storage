{
  "schemaVersion": 1,
  "id": "xp_storage",
  "version": "${version}",
  "name": "XP Storage",
  "description": "A mod that lets you store your experience in multiple ways!",
  "authors": [
    "Pardys"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/xp-storage",
    "issues": "https://github.com/charlyb01/xp_storage/issues",
    "sources": "https://github.com/charlyb01/xp_storage"
  },
  "license": "MIT",
  "icon": "assets/xp_storage/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.github.charlyb01.xpstorage.client.XpStorageClient"
    ],
    "main": [
      "com.github.charlyb01.xpstorage.XpStorage"
    ],
    "cardinal-components": [
      "com.github.charlyb01.xpstorage.component.MyComponents"
    ],
    "modmenu": [
      "com.github.charlyb01.xpstorage.config.ModMenuIntegration"
    ],
    "fabric-datagen": [
      "com.github.charlyb01.xpstorage.data.DataGenerator"
    ]
  },
  "mixins": [
    "xpstorage.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "fabric-api": ">=0.100.4"
  },
  "custom": {
    "cardinal-components": [
      "xp_storage:xp_component"
    ],
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/eNwkve8k7c"
      }
    }
  }
}
